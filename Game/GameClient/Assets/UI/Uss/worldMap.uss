/* 地图容器样式 */
.world-map-container {
    width: 800px; 
    height: 600px;
    border: 1px solid #000;
    overflow: hidden; /* 用于裁剪超出容器的地图内容，配合缩放拖动 */
}
/* 地图图片样式 */
.world-map-image {
    width: 100%;
    height: 100%;
}
/* 地标文本样式 */
.landmark-label {
    color: red;
    font-size: 14px;
    position: absolute; /* 绝对定位，方便根据坐标放置 */
}